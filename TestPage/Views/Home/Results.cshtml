    <tr><td>Image 2:</td><td tyle="text-align: right">@Convert.ToString(Model.Img2PHash, 2).PadLeft(64, '0')</td></tr>
    <tr><td>XOR:</td><td style="text-align: right">@Model.DiffrensePHash.PadLeft(64, '0')</td></tr>
    <tr><td>Hamming Distanse:</td><td style="text-align: right">@Model.HammingDistansePHash</td></tr>
    <tr><td>Result:</td><td style="text-align: right">@Model.PHashSummary</td></tr>
</table>

<h2>dHash based</h2>
