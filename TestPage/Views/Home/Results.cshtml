<table>
    <tr><td>Image 1:</td><td style="text-align: right">@Convert.ToString(Model.Img1Magic, 2).PadLeft(64, '0')</td></tr>
    <tr><td>Image 2:</td><td tyle="text-align: right">@Convert.ToString(Model.Img2Magic, 2).PadLeft(64, '0')</td></tr>
    <tr><td>XOR:</td><td style="text-align: right">@Model.DiffrenseMagic.PadLeft(64, '0')</td></tr>
    <tr><td>Hamming Distanse:</td><td style="text-align: right">@Model.HammingDistanseMagic</td></tr>
